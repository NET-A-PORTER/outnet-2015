${{name}}IconMap: (
  {{#shapes}}
    {{ base }}: (
      coordinates: {{ position.relative.xy }},
      width: {{ width.inner }}px,
      height: {{ height.inner }}px,
      selector: '{{#selector.shape}}{{expression}}{{/selector.shape}}'
    ),
  {{/shapes}}
);

${{name}}Icons: ({{#shapes}}{{#selector.shape}}'{{expression}}', {{/selector.shape}}{{/shapes}});
